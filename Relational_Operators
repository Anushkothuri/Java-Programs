// Relational Operators
class Relational_Operators
{
    public static void main(String[] args) 
    {
        int a=23;
        int b=49;
        System.out.println("a==b: "+(a==b));
        System.out.println("a!=b: "+(a!=b));
        System.out.println("a>b: "+(a>b));
        System.out.println("a<b: "+(a<b));
        System.out.println("a<=b: "+(a<=b));
        System.out.println("a>=b: "+(a>=b));
    }
}
